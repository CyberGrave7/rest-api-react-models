    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^12.12.7",
    "@types/react-redux": "^7.1.9",
    "fake-xml-http-request": "^2.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
