  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hector7/rest-api-react-models.git"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test",
      "coverage"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "author": "hector.rbs91@gmail.com",
  "license": "ISC",
  "homepage": "https://github.com/hector7/rest-api-react-models",
  "keywords": [
