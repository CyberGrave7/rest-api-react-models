  "dependencies": {
    "@rest-api/redux": "^2.0.0",
    "react-redux": "^7.2.0"
  },
  "peerDependencies": {
    "react-scripts": "^3.4.1",
    "typescript": "^3.9.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
